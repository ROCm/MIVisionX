data inter_luma = image:1280,720,U008
node org.khronos.openvx.channel_extract input_iyuv_1 !CHANNEL_Y inter_luma

data input_iyuv_2 = image:1280,720,IYUV:READ,inputs/stm_1280x720_copy.yuv

node org.khronos.openvx.channel_extract input_iyuv_2 !CHANNEL_Y inter_luma_2

data output_absdiff = image:1280,720,U008:WRITE,openvx_test_results/01_absdiff_1280x720.yuv
node org.khronos.openvx.absdiff inter_luma inter_luma_2 output_absdiff

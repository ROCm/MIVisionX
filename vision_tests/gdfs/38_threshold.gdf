
data input_iyuv = image:1280,720,IYUV:READ,inputs/stm_1280x720.yuv
data inter_luma = image:1280,720,U008
node org.khronos.openvx.channel_extract input_iyuv !CHANNEL_Y inter_luma

data output_threshold_image_bin = image:1280,720,U008:WRITE,openvx_test_results/38_threshold_1280x720.yuv
data input_threshold = threshold:BINARY,UINT8:INIT,127
node org.khronos.openvx.threshold inter_luma input_threshold output_threshold_image_bin

data output_threshold_image_range = image:1280,720,U008:WRITE,openvx_test_results/38_threshold_1280x720.yuv
data threshold_range = threshold:RANGE,UINT8:INIT,100,200
node org.khronos.openvx.threshold inter_luma threshold_range output_threshold_image_range